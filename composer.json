{
    "name": "triforce-technologies/symfony-rbac-bundle",
    "type": "symfony-bundle",
    "version": "0.9.21",
    "description": "Symfony Bundle allow to use RBAC control access for symfony project",
    "keywords": [
        "RBAC", "Security", "Access-Control", "Role", "phprbac.net", "owasp", "symfony", "bundle", "security"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Farid Anthony",
            "email": "farid.anthony@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2.1",
        "symfony/framework-bundle": "6.2.*",
        "webmozart/assert": "^1.11",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14"
    },
    "autoload": {
        "psr-4": { "Triforce\\RBACBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Triforce\\RBACBundle\\": "tests/" }
    }
}
